CREATE TABLE TB_PROFESSOR 
(
  PROFESSOR_NO VARCHAR2(10 BYTE) NOT NULL 
, PROFESSOR_NAME VARCHAR2(30 BYTE) NOT NULL 
, PROFESSOR_SSN VARCHAR2(14 BYTE) 
, PROFESSOR_ADDRESS VARCHAR2(100 BYTE) 
, DEPARTMENT_NO VARCHAR2(10 BYTE) 
, CONSTRAINT TB_PROFESSOR_PK PRIMARY KEY 
  (
    PROFESSOR_NO 
  )
  USING INDEX 
  (
      CREATE UNIQUE INDEX TB_PROFESSOR_PK ON TB_PROFESSOR (PROFESSOR_NO ASC) 
      LOGGING 
      TABLESPACE USERS 
      PCTFREE 10 
      INITRANS 2 
      STORAGE 
      ( 
        BUFFER_POOL DEFAULT 
      ) 
      NOPARALLEL 
  )
  ENABLE 
) 
LOGGING 
TABLESPACE USERS 
PCTFREE 10 
INITRANS 1 
STORAGE 
( 
  BUFFER_POOL DEFAULT 
) 
NOCOMPRESS 
NO INMEMORY 
NOPARALLEL;

COMMENT ON COLUMN TB_PROFESSOR.DEPARTMENT_NO IS 'TB_DEPARTMENT';
