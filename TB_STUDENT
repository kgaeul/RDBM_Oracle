CREATE TABLE TB_STUDENT 
(
  STUDENT_NO VARCHAR2(10 BYTE) NOT NULL 
, DEPARTMENT_NO VARCHAR2(10 BYTE) NOT NULL 
, STUDENT_NAME VARCHAR2(30 BYTE) NOT NULL 
, STUDENT_SSN VARCHAR2(14 BYTE) 
, STUDENT_ADDRESS VARCHAR2(100 BYTE) 
, ENTRANCE_DATE DATE 
, ABSENCE_YN CHAR(1 BYTE) 
, COACH_PROFESSOR_NO VARCHAR2(10 BYTE) 
, CONSTRAINT TB_STUDENT_PK PRIMARY KEY 
  (
    STUDENT_NO 
  )
  USING INDEX 
  (
      CREATE UNIQUE INDEX TB_STUDENT_PK ON TB_STUDENT (STUDENT_NO ASC) 
      LOGGING 
      TABLESPACE USERS 
      PCTFREE 10 
      INITRANS 2 
      STORAGE 
      ( 
        BUFFER_POOL DEFAULT 
      ) 
      NOPARALLEL 
  )
  ENABLE 
) 
LOGGING 
TABLESPACE USERS 
PCTFREE 10 
INITRANS 1 
STORAGE 
( 
  BUFFER_POOL DEFAULT 
) 
NOCOMPRESS 
NO INMEMORY 
NOPARALLEL;

COMMENT ON COLUMN TB_STUDENT.DEPARTMENT_NO IS 'TB_DEPARTMENT';

COMMENT ON COLUMN TB_STUDENT.COACH_PROFESSOR_NO IS 'TB_PROFESSOR';
