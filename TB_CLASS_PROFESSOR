CREATE TABLE TB_CLASS_PROFESSOR 
(
  CLASS_NO VARCHAR2(10 BYTE) NOT NULL 
, PROFESSOR_NO VARCHAR2(10 BYTE) NOT NULL 
, CONSTRAINT TB_CLASS_PROFESSOR_PK PRIMARY KEY 
  (
    CLASS_NO 
  , PROFESSOR_NO 
  )
  USING INDEX 
  (
      CREATE UNIQUE INDEX TB_CLASS_PROFESSOR_PK ON TB_CLASS_PROFESSOR (CLASS_NO ASC, PROFESSOR_NO ASC) 
      LOGGING 
      TABLESPACE USERS 
      PCTFREE 10 
      INITRANS 2 
      STORAGE 
      ( 
        BUFFER_POOL DEFAULT 
      ) 
      NOPARALLEL 
  )
  ENABLE 
) 
LOGGING 
TABLESPACE USERS 
PCTFREE 10 
INITRANS 1 
STORAGE 
( 
  BUFFER_POOL DEFAULT 
) 
NOCOMPRESS 
NO INMEMORY 
NOPARALLEL;

COMMENT ON COLUMN TB_CLASS_PROFESSOR.CLASS_NO IS 'TB_CLASS';

COMMENT ON COLUMN TB_CLASS_PROFESSOR.PROFESSOR_NO IS 'TB_PROFESSOR';
